    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "workbox-cli": "3.6.3"
  },
  "scripts": {
    "prestart": "npm run prepare:i18n",
    "start": "webpack-dev-server --env development",
    "start:build": "http-server dist -p 5000 -P http://localhost:8080 -o",
    "prebuild": "npm-run-all prepare:*",
    "prepare:tmp": "rm -rf .tmp",
    "prepare:i18n": "node scripts/merge-messages.js",
    "prepare:workbox": "workbox copyLibraries .tmp && mv .tmp/workbox-v$npm_package_devDependencies_workbox_cli .tmp/workbox",
    "build": "webpack --env production",
    "postbuild": "npm-run-all fix:*",
    "fix:elements": "replace '/elements/' '${window.location.pathname}/' dist/*.js",
    "fix:ui_elements": "replace '/node_modules/@nuxeo/nuxeo-ui-elements/' '${window.location.pathname}/' dist/*.js",
    "fix:pdfjs": "replace 'pdfjs/web/viewer.html' '../vendor/pdfjs/web/viewer.html' dist/*.js",
    "fix:cropper": "replace '/node_modules/cropperjs/dist/cropper.css' 'vendor/cropperjs/dist/cropper.css' dist/*.js",
    "fix:alloy": "replace 'alloy/lang/alloy-editor/' '../vendor/alloy/lang/alloy-editor/' dist/*.js",
    "fix:alloyfonts": "replace 'fonts/alloyeditor-ocean\\.' 'vendor/alloy/fonts/alloyeditor-ocean.' dist/*.js",
    "fix:fonts": "replace '\\.\\./fonts' './fonts' dist/*.js",
    "build:analyze": "webpack --env production --analyze",
    "lint:polymer": "polymer lint -i elements/**/*.js addons/**/*.js",
    "win-lint:polymer": "polymer lint -i elements\\**\\*.js addons\\**\\*.js",
    "format:polymer": "polymer lint -i elements/**/*.js addons/**/*.js --fix",
    "win-format:polymer": "polymer lint -i elements\\**\\*.js addons\\**\\*.js --fix",
    "lint:eslint": "eslint --ext .js,.html .",
    "format:eslint": "eslint --ext .js,.html . --fix",
    "lint:prettier": "prettier \"**/*.{js,html}\" --list-different || (echo '↑↑ these files are not prettier formatted ↑↑' && exit 1)",
    "format:prettier": "prettier \"**/*.{js,html}\" --write",
    "lint": "npm run lint:polymer && npm run lint:eslint && npm run lint:prettier",
    "win-lint": "npm run win-lint:polymer && npm run lint:eslint && npm run lint:prettier",
    "format": "npm run format:polymer && npm run format:prettier && npm run format:eslint",
    "win-format": "npm run win-format:polymer && npm run format:prettier && npm run format:eslint",
    "preinstall": "cd packages/nuxeo-web-ui-ftest && npm install",
    "test": "node scripts/test-runner.js --report --screenshots --headless --tags='not @ignore'",
    "test:dev": "node scripts/test-runner.js --nuxeoUrl=http://localhost:8080/nuxeo --url=http://localhost:5000/",
    "test:watch": "node scripts/test-runner.js --nuxeoUrl=http://localhost:8080/nuxeo  --url=http://localhost:5000/ --tags='@watch' --debug"
  },
  "dependencies": {
    "@nuxeo/chart-elements": "^4.0.0-SNAPSHOT",
