{
  "description": "Nuxeo Web UI",
  "homepage": "http://www.nuxeo.com",
  "name": "@nuxeo/nuxeo-web-ui",
  "version": "3.0.0-SNAPSHOT",
  "license": "Apache-2.0",
  "vendor": "Nuxeo",
  "devDependencies": {
    "@open-wc/webpack": "^0.1.6",
    "@webcomponents/webcomponentsjs": "^2.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "eslint-plugin-html": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "polymer-cli": "^1.9.0",
    "replace": "^1.0.1",
    "webpack": "~4.28.4",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-merge": "^4.1.4",
    "workbox-cli": "3.6.3"
  },
  "scripts": {
    "prestart": "npm run prepare:i18n",
    "start": "webpack-dev-server --env development",
    "prebuild": "npm-run-all prepare:*",
    "prepare:tmp": "rm -rf .tmp",
    "prepare:i18n": "node scripts/merge-messages.js",
    "prepare:workbox": "workbox copyLibraries .tmp && mv .tmp/workbox-v$npm_package_devDependencies_workbox_cli .tmp/workbox",
    "build": "webpack --env production",
    "postbuild": "npm-run-all fix:*",
    "fix:elements": "replace '/elements/' '${window.location.pathname}/' target/classes/web/nuxeo.war/ui/*.js",
    "fix:ui_elements": "replace '/node_modules/@nuxeo/nuxeo-ui-elements/' '${window.location.pathname}/' target/classes/web/nuxeo.war/ui/*.js",
    "fix:fonts": "replace '\\.\\./fonts' './fonts' target/classes/web/nuxeo.war/ui/*.js",
    "build:analyze": "webpack --env production --analyze",
    "lint": "gulp lint && polymer lint -i elements/*/*.html",
    "win-lint": "gulp lint && polymer lint -i elements\\*\\*.html"
  },
  "engines": {
    "node": ">=8.0"
  },
  "dependencies": {
    "@nuxeo/chart-elements": "^4.0.0-SNAPSHOT",
    "@nuxeo/nuxeo-dataviz-elements": "^3.0.0-SNAPSHOT",
    "@nuxeo/nuxeo-elements": "^3.0.0-SNAPSHOT",
    "@nuxeo/nuxeo-ui-elements": "^3.0.0-SNAPSHOT",
    "@nuxeo/page": "^1.11.4-SNAPSHOT",
    "@polymer/app-layout": "^3.0.0",
    "@polymer/iron-a11y-keys": "^3.0.0",
    "@polymer/iron-icons": "^3.0.0",
    "@polymer/iron-jsonp-library": "^3.0.0",
    "@polymer/iron-label": "^3.0.0",
    "@polymer/iron-localstorage": "^3.0.0",
    "@polymer/iron-location": "^3.0.0",
    "@polymer/iron-media-query": "^3.0.0",
    "@polymer/iron-overlay-behavior": "^3.0.0",
    "@polymer/iron-pages": "^3.0.0",
    "@polymer/iron-validator-behavior": "^3.0.0",
    "@polymer/marked-element": "^3.0.0",
    "@polymer/neon-animation": "^3.0.0",
    "@polymer/paper-badge": "^3.0.0",
    "@polymer/paper-dialog-scrollable": "^3.0.0",
    "@polymer/paper-drawer-panel": "^3.0.0",
    "@polymer/paper-fab": "^3.0.0",
    "@polymer/paper-header-panel": "^3.0.0",
    "@polymer/paper-listbox": "^3.0.0",
    "@polymer/paper-radio-button": "^3.0.0",
    "@polymer/paper-radio-group": "^3.0.0",
    "@polymer/paper-slider": "^3.0.0",
    "@polymer/paper-tabs": "^3.0.0",
    "@polymer/paper-toolbar": "^3.0.0",
    "@polymer/polymer": "^3.0.0",
    "@webcomponents/html-imports": "^1.2.0",
    "d3": "^4.13.0",
    "jsondiffpatch": "^0.3.11",
    "jsplumb": "^2.9.0",
    "web-animations-js": "^2.3.1"
  }
}
