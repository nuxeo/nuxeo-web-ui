    "css-loader": "^2.1.1",
    "cucumber": "^4.1.0",
    "eslint": "^5.15.1",
    "eslint-plugin-html": "^5.0.3",
    "glob": "^7.1.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.11.1",
    "husky": "^1.0.0",
