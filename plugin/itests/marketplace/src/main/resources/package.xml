<package type="addon" name="nuxeo-web-ui-ftest-sample" version="@VERSION@">
  <title>Nuxeo Web UI</title>
  <description>
    <p>Web UI sample extension for functional tests</p>
  </description>
  <home-page>https://github.com/nuxeo/nuxeo-web-ui</home-page>
  <vendor>Nuxeo</vendor>
  <installer restart="true" />
  <uninstaller restart="true" />
  <nuxeo-validation>nuxeo_certified</nuxeo-validation>
  <production-state>proto</production-state>
  <supported>false</supported>
  <platforms>
    <platform>server-@NUXEO_PLATFORM_VERSION@</platform>
    <platform>server-@NUXEO_PLATFORM_VERSION@-*</platform>
    <platform>server-@NUXEO_TARGET_VERSION@</platform>
  </platforms>
  <dependencies>
    <package>nuxeo-web-ui</package>
  </dependencies>
  <license>Apache License, Version 2.0</license>
  <license-url>http://www.apache.org/licenses/LICENSE-2.0.html</license-url>
  <visibility>PRIVATE</visibility>
</package>
