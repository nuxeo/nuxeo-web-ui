{
  "entrypoint": "index.html",
  "fragments": [
    "elements/elements.html"
  ],
  "sources": [
    "elements/**/*",
    "themes/**/*",
    "images/**/*",
    "bower.json"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*",
    "bower_components/page/page.js",
    "bower_components/moment/min/moment-with-locales.min.js",
    "bower_components/nuxeo-ui-elements/viewers/pdfjs/*",
    "bower_components/nuxeo-ui-elements/widgets/alloy/**/*"
  ],
  "lint": {
    "rules": ["polymer-1"],
    "ignoreWarnings": [
      "polymer-expression-parse-error",
      "databind-with-unknown-property",
      "set-unknown-attribute"
    ]
  }
}
